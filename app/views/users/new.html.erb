<%= error_messages_for :user %>
<% form_for @user, :html => { :multipart => true } do |f| -%>
	<p><label for="login">로그인ID</label><br/>
	<%= f.text_field :login, :class => 'required', :minlength => 4 %></p>
	<%= render :partial => 'form', :object => f %>
<p><span class="button"><%= submit_tag '만들기' %></span></p>
<% end -%>
